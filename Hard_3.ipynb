      "source": [
        "def count_digit_one(n):\n",
        "    count = 0\n",
        "\n",
        "    for i in range(1, n + 1):\n",
        "        count += str(i).count('1')\n",
        "\n",
        "    return count\n",
        "\n",
        "\n",
        "n=int(input(\"enter a number\"))\n",
        "print(count_digit_one(n))"
      ]
